
@model Skladiste.Models.Artikal
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>View</h1>
<form>
    <div>@Model.Naziv</div>
    <div>@Model.Jedinica_Mjere</div>
    <div>@Model.Kolicina</div>
    <div class="form-group">
        <label class="control-label">Ulaz</label>
        <input id="Ulaz" asp-for="Izlaz" class="form-control" />
        <button type="submit" value="Add" asp-controller="Izlaz" asp-action="Index" asp-route-id="@Model.Id" name="Save" formmethod="post">Pohrani</button>
    </div>
</form>


